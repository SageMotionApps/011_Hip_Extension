{
  "user_guide_file": "Hip Extension App Guide.pdf",
  "app_name": "Hip Extension",
  "app_description": "Measure and train left or right hip extension angle <br><br>Nodes Required: 4 <br> -Sensing (2): pelvis (back center, switch pointing up), thigh (left or right lateral, switch pointing up) <br> -Feedback (2): feedback_min, feedback_max",
  "app_id": "011",
  "app_version": "1_2",
  "version_id": "01112",
  "validated_version_range": [
    "3.20.1",
    "4.1.0"
  ],
  "parameters_setting": [
    [
      "Sensor Placement",
      {
        "source": "which_leg",
        "title": "Which Leg?",
        "type": "combo",
        "validation": [
          "Right Leg",
          "Left Leg"
        ],
        "description": "Choose the leg you want to place the sensor"
      }
    ],
    [
      "Feedback Setting",
      {
        "source": "feedback_enabled",
        "title": "Enable Feedback",
        "type": "bool",
        "description": "Enable/Disable feedback"
      },
      {
        "source": "min_threshold",
        "title": "Min Threshold Angle",
        "type": "float",
        "validation": [
          -90,
          90
        ],
        "description": "Feedback given when angle is less than this value"
      },
      {
        "source": "max_threshold",
        "title": "Max Threshold Angle",
        "type": "float",
        "validation": [
          -90,
          90
        ],
        "description": "Feedback given when angle is greater than this value"
      }
    ],
    [
      "Save Options",
      {
        "source": "save_mode",
        "title": "Save Mode",
        "type": "combo",
        "validation": [
          "csv",
          "h5",
          "xlsx"
        ],
        "description": "Save file in specified format"
      }
    ]
  ],
  "user_fields": [
    {
      "name": "time",
      "data_type": "float",
      "size": 1
    },
    {
      "name": "Hip_ext",
      "data_type": "float",
      "size": 1
    },
    {
      "name": "max_threshold",
      "data_type": "float",
      "size": 1
    },
    {
      "name": "min_threshold",
      "data_type": "float",
      "size": 1
    },
    {
      "name": "min_feedback_state",
      "data_type": "int",
      "size": 1
    },
    {
      "name": "max_feedback_state",
      "data_type": "int",
      "size": 1
    }
  ],
  "sensors": [
    "thigh",
    "pelvis"
  ],
  "feedback": [
    "feedback_min",
    "feedback_max"
  ],
  "Is_pull": true,
  "pulse_length": 1,
  "datarate": 100
}